{
  "name": "flipping",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@onflow/fcl": "^1.3.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "framer-motion": "^10.12.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "REACT_APP_FLOW_NETWORK=local react-scripts start",
    "start:testnet": "REACT_APP_FLOW_NETWORK=testnet react-scripts start",
    "build": "REACT_APP_FLOW_NETWORK=mainnet react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:widget": "REACT_APP_FLOW_NETWORK=mainnet parcel build src/index.js --no-source-maps --dist-dir widget && cp build/static/css/*.css widget/index.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "buffer": "^5.5.0",
    "parcel": "^2.8.3"
  }
}
